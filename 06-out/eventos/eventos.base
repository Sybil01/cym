filters:
  and:
    - file.folder == "06-out/eventos"
    - file.ext != "base"
views:
  - type: cards
    name: Table
    order:
      - file.name
      - year
      - city
      - place
      - curators
    sort:
      - property: year
        direction: ASC
    image: note.photo
    imageFit: ""
    imageAspectRatio: 0.5
    cardSize: 320
